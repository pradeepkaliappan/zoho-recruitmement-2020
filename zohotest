import java.util.Scanner;

public class zohoTest {

	public static void main(String[] args) {
		Scanner input= new Scanner(System.in);
		int inputValue = 0;
		inputValue = input.nextInt();
		int space_count = 1;
		
		for(int i=inputValue;i>0;i--) {
			for(int j=inputValue;j>0;j--) {
				if(j<=i)
					System.out.print(i);
				else
					System.out.print(j);
				if(space_count<inputValue)
					System.out.print(" ");
				space_count++;
			}System.out.println();
			space_count = 1;
		}
		
		

	}

}
